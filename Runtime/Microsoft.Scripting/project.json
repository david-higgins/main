{
  "version": "1.1.1.22",
  "buildOptions": {
    "compile": {
      "include": [ "../../Hosts/Silverlight/SilverlightVersion.cs" ],
      "exclude": [ "Runtime/DynamicOperations.Generated.cs" ]
    },
    "define": [ "TRACE" ],
    "platform": "anycpu"
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "debugType": "full",
        "define": [ "DEBUG" ],
        "keyFile": "../../Solutions/DevelKey.snk"
      }
